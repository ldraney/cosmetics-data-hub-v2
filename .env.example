# Environment Variables Example
# Copy this file to .env.local for local development

# Database Configuration
# For local development with Docker: postgres://webhook_user:webhook_password@localhost:5432/webhook_db
# For Fly.io proxy: postgres://username:password@localhost:15432/database_name
# Production: Set automatically by Fly.io
DATABASE_URL=postgres://webhook_user:webhook_password@localhost:5432/webhook_db

# Application Environment
NODE_ENV=development

# Server Port (optional, defaults to 3000)
PORT=3005